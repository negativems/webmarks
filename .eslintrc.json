{
   "root": true,
   "env": {
      "browser": true,
      "node": true
   },
   "parser": "vue-eslint-parser",
   "parserOptions": {
      "parser": "@typescript-eslint/parser"
   },
   "rules": {
      "no-irregular-whitespace": [
         "off"
      ],
      "semi": [
         2,
         "always"
      ],
      "no-unused-vars": "off",
      "@typescript-eslint/explicit-function-return-type": [
         "off"
      ]
   }
}